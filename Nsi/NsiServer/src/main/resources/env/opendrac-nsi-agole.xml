<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE properties SYSTEM "http://java.sun.com/dtd/properties.dtd">

<properties>
    <comment>OpenDRAC NSI Configuration for AGOLE</comment>

    <!-- NSA configuration information including peer NSA. -->
    <entry key="nsaConfigurationFile">conf/nsi-nsa-configuration.xml</entry>

    <!-- The SOAP endpoint for my Requester endpoint. -->
    <entry key="nsaRequesterEndpoint">http://dracproxy01.surfnet.nl:8080/nsi-v1/ConnectionServiceRequester</entry>

    <!-- Global topology file for routing. -->
    <entry key="topologyFile">conf/AutoGOLE-Topo-2012-02-29.owl</entry>

    <!-- Individual actor pool size. -->
    <entry key="actorPoolSize">10</entry>

    <!-- Local OpenDRAC NRM authentication/request information. -->
    <entry key="nrmUserName">nsidemo</entry>
    <entry key="nrmPassword">RioPlug-Fest2011!</entry>
    <entry key="nrmBillingGroup">NsiUserGroup</entry>
    <entry key="nrmEndpointUserGroup">NsiUserGroup</entry>
    <entry key="nrmEndpointResourceGroup">NsiResourceGroup</entry>

</properties>
